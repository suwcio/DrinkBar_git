<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    
    <navigation-rule>
        <navigation-case>
            <from-action>pokazIndex</from-action>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-action>pokazDrinki</from-action>
            <to-view-id>/drinki.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-action>pokazDodajPrzepis</from-action>
            <to-view-id>/dodajprzepis.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-action>pokazSlowniczek</from-action>
            <to-view-id>/slowniczek.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-action>pokazKontakt</from-action>
            <to-view-id>/kontakt.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
